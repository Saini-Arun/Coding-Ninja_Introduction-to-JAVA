public class solution {
	public static void print2DArray(int input[][]) {
		
		int length=input.length;
		int colmlength=input[0].length;
		int row=0; 
		int turn=length;
		for (int n=turn;n>0;n--) {
			int t=n;
		while(t>0) {
		for(int i=0;i<colmlength;i++) {
			System.out.print(input[row][i]+" ");
		}System.out.println();
		t--;
	}
		row++;
		}

	}
}
